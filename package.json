    "@types/react-transition-group": "^2.0.8",
    "@types/redux": "^3.6.0",
    "@types/resolve": "0.0.7",
    "@types/webpack": "^4.1.3",
    "@types/webpack-dev-server": "^2.9.4",
    "@types/webpack-manifest-plugin": "^1.3.2",
    "@types/whatwg-fetch": "0.0.33",
    "ajv": "^6.4.0",
